print("y = mx+ b where b is the y coordinate and x is...the x coordinates, m = slope")
print("Integers only, no floats ")
x_values = int(input("enter values for x: "))

def my_slope(x1,y1,x2,y2):
    y_values = y2-y1
    x_values = x2-x1
    m = y_values / x_values
    return m
def my_coordinates():
